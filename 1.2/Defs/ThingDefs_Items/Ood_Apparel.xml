<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase">
    <defName>Apparel_Ood</defName>
    <description>A simple jumpsuit, tagged with the trademark of Ood Industries, a 39th Century Corporation</description>
    <label>jumpsuit</label>
    <recipeMaker>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Pawns/Ood/Jumpsuit</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <costStuffCount>80</costStuffCount>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
    <thingCategories>
      <li>Apparel</li>
    </thingCategories>
    <statBases>
      <MaxHitPoints>200</MaxHitPoints>
      <WorkToMake>22000</WorkToMake>
      <Mass>2.2</Mass>
      <ArmorRating_Blunt>0.03</ArmorRating_Blunt>
      <ArmorRating_Sharp>0.07</ArmorRating_Sharp>
      <ArmorRating_Heat>0.07</ArmorRating_Heat>
      <Insulation_Cold>15</Insulation_Cold>
      <Insulation_Heat>15</Insulation_Heat>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.04</MoveSpeed>
    </equippedStatOffsets>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Shoulders</li>
        <li>Arms</li>
        <li>Legs</li>
      </bodyPartGroups>
      <wornGraphicPath>Pawns/Ood/Jumpsuit</wornGraphicPath>
      <layers>
        <li>Shell</li>
      </layers>
      <tags>
        <li>OodJumpsuit</li>
      </tags>
      <defaultOutfitTags>
        <li>OodJumpsuit</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
</Defs>